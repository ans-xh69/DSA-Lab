#include <iostream>
#include "list.cpp"

using namespace std;

int main() {
	List <int> l;
	l.push(5);
	l.push(23);
	cout << "Popping:\n";
	cout << l.pop() << endl;
	cout << l.pop() << endl;

	cout << "\nTesting insertAfter():\n";
	l.push(100);
	l.push(77);
	l.insertAfter(100, 27);  //Inserted 27 after 100

	while (!l.emptyList()) {
		cout << l.pop() << endl;
	}

	cout << "\nTesting deletItem():\n";
	l.push(1);
	l.push(2);
	l.push(3);
	l.deleteItem(2);

	while (!l.emptyList()) {
		cout << l.pop() << endl;
	}

	return 0;
}